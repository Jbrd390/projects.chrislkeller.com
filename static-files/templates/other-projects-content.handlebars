<div class="row-fluid">
    <div class="span3">
        <h4>Projects</h4>
            {{#objects}}
                {{#is projecttype 'projects'}}
                    <ul>
                        {{#is projecturl 'n/a'}}
                            <li><a href="{{projectrepo}}" target="_blank">{{projecttitle}}</a></li>
                        {{else}}
                            <li><a href="{{projecturl}}" target="_blank">{{projecttitle}}</a></li>
                        {{/is}}
                    </ul>
                {{else}}
                {{/is}}
            {{/objects}}
    </div>
